{
  "name": "@expense-tracker/infrastructure",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist cdk.out && del-cli *.tsbuildinfo",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "destroy": "cdk destroy --all"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "aws-cdk": "^2.115.0",
    "aws-cdk-lib": "^2.230.0",
    "constructs": "^10.4.3",
    "esbuild": "^0.27.0",
    "typescript": "^5.3.0"
  }
}